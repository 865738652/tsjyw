/*弹窗登录*/
* { margin:0; padding:0;}
img { border:none 0;}
#btn_center { position:relative;}
.window{ z-index:1000; position:fixed; _position:absolute; bottom:0; right:20px; _bottom:auto; _top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||0)-(parseInt(this.currentStyle.marginBottom,10)||0))); top:50%; left:50%; width:390px; height:230px; display:none; border:2px solid #d16719; background:#FFF url(../images/images/pop_logo.jpg) no-repeat 18px 8px; } 
.window ul { width:98%; margin:0 auto; list-style:none; }
.window ul li { height:31px; line-height:31px; padding-left:10px; border-bottom:1px dashed #ccc;}
.window ul li a { font-size:12px; color:#666; text-decoration:none;}
.window ul li a:hover { color:#9b3202;}
.window ul li a.close { display:block; float:right; margin-top:3px; }
.pop_page { padding-left:10px; line-height:36px;}
.pop_page a, .pop_page span { margin:0 5px; font-size:12px; color:#cc6e16; text-decoration:none;}
.pop_page span { color:#333;}
.pop_page a:hover { color:#f00;}