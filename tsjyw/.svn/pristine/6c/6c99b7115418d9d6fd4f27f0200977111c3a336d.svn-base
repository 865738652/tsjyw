/* 搜索 */
.search{position: relative;left:0;top:0; height: 45px;background-color: #fff;border-top: 1px solid #e5e5e5;border-bottom: 1px solid #e5e5e5;margin: 13px 0;}
.search input[type='submit']{position: absolute;right: 0;top:0;width: 50px;height: 45px;border: 0;border-left: 1px solid #d5d5d5;background: url(data:image/png;base64,) no-repeat center center;-webkit-background-size: 20px 18px;background-size: 20px 18px;}
.search .cancel{position: absolute;top:10px;right: 55px;width: 30px;height: 25px;border: 0;padding: 2px 0 0;background-color: #fff;}
.search .cancel i{display: inline-block;width: 20px;height: 20px;background: url(data:image/png;base64,) no-repeat center;-webkit-background-size: 20px 20px;background-size: 20px 20px;}

/* 焦点图 focus */
.focus { width: 320px; height: 160px; position: relative; margin: 13px auto;overflow: hidden; }
.focus img { display: block; }
.focus .switch { position: absolute; left: 0; bottom: 0; width: 100%; height: 30px; line-height: 30px; background: rgba(0,0,0,0.5); }
.focus .text { width: 245px; color: #fff; font-size: 16px; display: inline-block; padding: 0 9px; float: left; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }
.focus .tabs { width: 45px; height: 8px; float: left; _display: inline; margin: 11px 0 0 10px; }
.focus .tabs a { width: 8px; height: 8px; border-radius: 4px; background-color: #949493; cursor: pointer; text-indent: -9999em; margin-right: 7px; float: left; }
.focus .tabs .current { background-color: #d60000; }

.focus-pic { width: 320px; height: 160px; }
.focus-pic li { float: left; width: 320px; height: 160px; margin-right: 10px;}

