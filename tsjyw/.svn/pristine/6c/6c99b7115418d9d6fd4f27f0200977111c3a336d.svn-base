.photo-pic {
	width: 16rem;
	margin: 0 auto;
}
.photo-pic .c-1 {
	overflow: hidden;
	padding: 0 0 0.5rem 0;
	width: 16rem;
}
.photo-pic .one {
	width:10.5rem;
	float: left;
	position: relative;
	display: inline-block;
}
.photo-pic .one img {
	width: 100%;
}
.photo-pic .first-txt {
	position: absolute;
	left: 6px;
	bottom: 6px;
	padding: 0 5px;
	height: 32px;
	line-height: 32px;
	font-size: 25px;
	background-color: rgba(0,0,0,0.5);
}
.photo-pic .c-1-right {
	width:5rem;
	float: right;
	position: relative;
}
.photo-pic .c-1-right .two {
	width: 100%;
	height: 88px;
	line-height: 40px;
	padding-top: 10px;
	color: #fff;
	font-size: 34px;
	background-color: #14aebd;
	text-align: center;
}
.photo-pic .c-1-right .two img {
	width: 100%;
}
.photo-pic .c-1-right .two-2 {
	width: 100%;
	position: relative;
	display: inline-block;
	vertical-align: middle;
	margin: 0 0 0.5rem 0;
}
.photo-pic .c-1-right .two-2 img {
	width: 100%;
}
.photo-pic .c-1-right .three {
	width: 100%;
	position: relative;
	display: inline-block;
	vertical-align: middle;
}
.photo-pic .c-1-right .three img {
	width: 100%;
}
.photo-pic .c-3 {
	overflow: hidden;
	padding: 0 0 0.5rem 0;
}
.photo-pic .c-3 a {
	width: 5rem;
	float: left;
	margin: 0 0.5rem 0 0;
	position: relative;
	display: inline-block;
}
.photo-pic .c-3 a:nth-of-type(3) {
	margin: 0;
}
.photo-pic .c-3 a img {
	width: 100%;
}
    
.photo-pic .txt{position: absolute; left: 6px; bottom: 6px; padding: 0 5px; height: 20px;line-height: 20px;font-size: 16px;background-color:rgba(0,0,0,0.5);}
.photo-pic a{display: block;overflow: hidden;color: #fff;text-align: center;background-color: #333;}

