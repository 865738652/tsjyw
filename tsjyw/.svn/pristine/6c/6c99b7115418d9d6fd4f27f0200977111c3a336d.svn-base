/* activity-center-section */
.activity-center-section{}
.activity-center-section .section-header{background-color: #f1f1f1;height: 39px;line-height: 39px;}
.activity-center-section .section-header .section-title{display: inline-block; margin: -1px 0 0 10px; padding: 0 10px 0 35px; border-top: 1px solid #0eb08f; background: #fff url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAC4AAAAnAgMAAAAN2JJsAAAADFBMVEUOsI8cmaX///8OsI+YDF1DAAAAA3RSTlMAAAD6dsTeAAAAUUlEQVR4XmP4DwdZC1A5HxiggB+d8/n/f3ugmg8M/AwMDJ+ZmQ0YGBgMwByEjP0HhB4gh+E/HDCgcRDKgJxRDr6gQgAgZ9X6VSuI4sBNQOUAANAgo6SGIfYuAAAAAElFTkSuQmCC) no-repeat 6px 50%; -webkit-background-size: 23px 20px; background-size: 23px 20px;}
.activity-center-section .section-title a{color:#0eb08f;}

.activity-list{padding: 0 10px;}
.activity-list .activity-item{border-bottom: 1px solid #d8d8d8;}
.activity-list .activity-item a{display: block;}
.activity-list .activity-item .activity-title{display: block; float: left; width: 100%; overflow : hidden; text-overflow: ellipsis; display:-webkit-box; -webkit-line-clamp:2; -webkit-box-orient: vertical; padding: 16px 0 5px; font-size: 16px; line-height: 24px;}
.activity-list .activity-item .activity-title .tag{color: #0eb08f;}
.activity-list .activity-item .activity-infor{clear: both;}
.activity-list .activity-item .pic{float: left; width: 120px; height: 80px; margin: 4px 10px 0 0;}
.activity-list .activity-item .pic img{width: 120px; height: 80px; vertical-align: middle;}
.activity-list .activity-item p{overflow: hidden; zoom: 1; height: 22px; color: #999; font-size: 14px; line-height: 22px;}
.activity-list .activity-item p span{color: #666;}
.activity-list .activity-item .price{color: #fb801e;}
.activity-list .activity-item .name{color: #999;}
.activity-list .activity-item .name.vip:after{content: ''; display: inline-block; width: 14px; height: 14px; overflow: hidden; margin: -1px 0 0 5px; background: url(data:image/png;base64,) no-repeat 50% 50%; -webkit-background-size: 14px 14px; background-size: 14px 14px; vertical-align: middle; font: 0/0 arial;}
.activity-list .activity-item .toolbar{height: 20px; overflow: hidden; padding: 10px 0; line-height: 20px;}
.activity-list .activity-item .num{float: right; color: #666; font-size: 14px;}
.activity-list .activity-item .num em{color: #fb801e; font-size: 14px;}
.activity-list .activity-item .timer{float: left; color: #666; font-size: 14px;}
.activity-list .activity-item .timer em{color: #fb801e; font-size: 14px;}
.activity-list .activity-item .clock-icon{float: left; width: 16px; height: 16px; overflow: hidden; margin: 1px 5px 0 0; background: url(data:image/png;base64,) no-repeat 0 50%; -webkit-background-size: 16px 16px; background-size: 16px 16px;}
.activity-list .activity-item .timer-clock{display: none; font-size: 14px;}
.activity-list .activity-item .unStart .timer-text{display: none;}
.activity-list .activity-item .unStart .timer-clock{display: block;}
/**
 *新加wap  pe页样式
 */
/*摄影师大全*/
.y-wap-photographer{margin: 15px 0 0 0;}
.y-wap-photographer .btn li{width: 33.3333333333334%; float: left; border-top: 3px #f0f0f0 solid; background: #f0f0f0; height: 48px; line-height: 48px; font-size: 16px; text-align: center;}
.y-wap-photographer .btn li.active{border-top: 3px #fe5740 solid; background: #fff;}
.y-wap-photographer .content{padding: 5px;}
.y-wap-photographer .content li{width: 33.3333333333334%; float: left;}
.y-wap-photographer .content li a{display: block; padding: 5px;}
.y-wap-photographer .content li img{width: 100%; height: auto;}
.y-wap-photographer .content li p{font-size: 16px; text-align: center; color: #333; line-height: 30px;}
/*活动详情*/
.y-wap-activity .back{height: 40px; line-height: 40px; background: #f7f7f7; font-size: 16px; color: #666;}
.y-wap-activity .back a{padding: 0 0 0 10px;}
.y-wap-activity .crumb{border-bottom: 1px #d6d6d6 solid; margin: 0 10px;}
.y-wap-activity .title{color: #333; font-size: 15px; line-height: 23px; padding: 10px;}
.y-wap-activity .title font{color: #fe5740;}
.y-wap-activity .time{padding: 0 10px; color: #999; font-size: 13px;}
.y-wap-activity .content{padding: 0 10px;}
.y-wap-activity .content p{margin: 15px 0 0 0; color: #333; line-height: 23px; font-size: 15px;}
.y-wap-activity .content span{margin: 15px 0 0 0; color: #999; display: block;}
.y-wap-activity .good-bad{margin: 5px 0 0 0;}
.y-wap-activity .good-bad a{color: #14aebd; font-size: 13px; line-height: 22px; margin: 0 15px 0 0;}
.y-wap-activity .good-bad a:nth-of-type(1){color: #fff; padding: 5px 7px 5px 23px; background:url("../images/y_wap_icon_good&bad.png") 7px 7px no-repeat #fb6c76; background-size: 12px auto; border-radius: 22px;}
.y-wap-activity .good-bad a:nth-of-type(1).active{background-color: #ee424d;}
.y-wap-activity .good-bad a:nth-of-type(2){color: #fff; padding: 5px 7px 5px 23px; background:url("../images/y_wap_icon_good&bad.png") 7px -18px no-repeat #fcca74; background-size: 12px auto; border-radius: 22px;}
.y-wap-activity .good-bad a:nth-of-type(2).active{background-color: #f6b139;}
.y-wap-activity .search{position: relative; margin: 20px 0 0 0; height: 45px; border: 1px #d5d5d5 solid;}
.y-wap-activity .search input[type='text']{height: 45px; border: 0; padding: 0 10px; width: 100%;}
.y-wap-activity .list{margin: 20px 0 0 0;}
.y-wap-activity .list span{display: block; padding: 0 10px; background: #f7f7f7; height: 35px;}
.y-wap-activity .list span font{border-top: 1px #fe5740 solid; height: 34px; line-height: 34px; background: #fff; padding: 0 10px; display: inline-block;}
.y-wap-activity .list ul{padding: 0 10px;}
.y-wap-activity .list li{border-bottom: 1px #d6d6d6 solid; line-height: 35px; color: #333; font-size: 13px;}
/*列表*/
.y-wap-title-box{height: 36px; background: #f1f1f1; padding: 0 10px;}
.y-wap-title-box span{font-size: 18px; line-height: 35px; color: #fe5740; border-top: 1px #fe5740 solid; background-color: #fff; background-position-x: 10px; background-repeat: no-repeat; background-image: url("../images/y_wap_icon.png"); background-size: 20px auto; display: inline-block; height: 35px; padding: 0 10px 0 35px;}
.y-wap-faq-list{padding: 0 10px;}
.y-wap-faq-list li{padding: 10px 0; border-bottom: 1px #e9e9e9 solid;}
.y-wap-faq-list li p{line-height: 30px; font-size: 15px; overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.y-wap-faq-list li p font{float: none; color: #fe5740;}
.y-wap-faq-list li p a{color: #333; padding: 0; background: none;}
.y-wap-faq-list li font{float: right; color: #999; font-size: 13px; line-height: 30px;}
.y-wap-faq-list li a{line-height: 30px; color: #fe5740; font-size: 13px; padding: 0 0 0 15px; background: url("../images/y_wap_icon_good&bad.png") 0 -72px no-repeat; background-size: 12px auto;}
.y-wap-faq-list li a.red{ background: url("../images/y_wap_icon_good&bad.png") 0 -46px no-repeat; background-size: 12px auto;}
.y-wap-classify-list{padding: 10px; text-align: center;}
.y-wap-classify-list li{width: 24%; float: left; line-height: 30px; padding: 0 1% 0 0; text-align: left; overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.y-wap-classify-list li a{font-size: 13px; color: #333; padding: 0;}
.y-wap-classify-list li.active a{color: #fe5740;}
.y-wap-classify-list a{font-size: 14px; color: #fe5740; position: relative; padding: 0 0 0 20px;}
.y-wap-classify-list a font{transform: rotate(90deg); display: block; line-height: 19px; width: 19px; position: absolute; left: 0; top: 3px;}
.y-wap-library-list .btn li{width: 50%; height: 45px; line-height: 45px; font-size: 16px; text-align: center; border-bottom: 1px #fff solid; float: left;}
.y-wap-library-list .btn li.active{border-bottom: 1px #fe5740 solid; color: #fe5740;}
.y-wap-library-list .content{display: none; padding: 10px;}
.y-wap-library-list .content li{width: 33.3333333333334%; float: left; line-height: 30px; font-size: 13px; color: #333; overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.y-wap-library-list .content span{display: block; height: 0; margin: 15px 0; position: relative; border-bottom: 1px #ccc dashed;}
.y-wap-library-list .content span font{background: #fff; width: 50px; height: 20px; position: absolute; top: 50%; left: 50%; margin: -10px 0 0 -25px; text-align: center; line-height: 20px; color: #fe5740; font-size: 16px;}
.faq span{background-position-y: -32px;}
.classify span{background-position-y: 10px;}
.equipment span{background-position-y: -75px;}
.library span{background-position-y: -117px;}
.bdcs-container{margin-bottom: 15px;margin-top: 10px;}
.more-result{display: block; text-align: center; padding: 10px 0;}
.more-result .list_more{color: #fe5740; font-size: 14px;}
.search-az{width: 310px; position: fixed; right: 10px; bottom: 150px; background: #ccc; z-index: 9;}
.search-az a{width: 34px; text-align: center; display: block; float: left; margin: 5px 0;}
.search-az span{display: block; padding: 10px 0; text-align: center; font-size: 18px;}
#close{position: absolute; top: 5px; right: 5px; background: #fff; margin: 0; width: 30px; height: 30px; border-radius: 30px; text-align: center; line-height: 26px;}
.search-az-btn{width: 80px; height: 80px; z-index: 9; background: #14aebd; border-radius: 80px; position: fixed; right: 10px; bottom: 70px; color: #fff; font-size: 20px; text-align: center; line-height: 75px;}
