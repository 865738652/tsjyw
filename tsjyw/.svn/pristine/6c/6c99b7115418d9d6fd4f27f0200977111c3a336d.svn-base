.page-list {
    width: 308px;
    height: 32px;
    margin-left: auto;
    margin-right: 6px;
    background: #f8f8f8;
    border: solid #d0d6e1 1px;
    border-radius: 3px;
    font-size: 16px;
    line-height: 1;
    overflow: hidden;
    margin: 12px auto;
}
.page-list a {
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    float: left;
    padding: 8px 0;
    border-right: solid #d0d6e1 1px;
    color: #14aebd;
    text-align: center
}
.page-list a.home {
    width: 17%
}
.page-list a.prev {
    width: 22%
}
.page-list a.next {
    width: 22%
}
.page-list a.last {
    width: 17%;
    border-right: none
}
.page-list a:visited {
    color: #14aebd
}
.page-list a.disabled {
    color: #a2a6ae
}
.page-list span {
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    float: left;
    padding: 8px 0;
    border-right: solid #d0d6e1 1px;
    text-align: center
}
.page-list span.num {
    width: 22%;
    padding: 0 6px;
    height: 32px
}
.page-list span.num input {
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    width: 100%;
    margin: 2px 0 0;
    padding: 3px 0;
    background-image: linear-gradient(top, #e7e9ee, #f5f7f9);
    background-image: -webkit-linear-gradient(top, #e7e9ee, #f5f7f9);
    background-image: -moz-linear-gradient(top, #e7e9ee, #f5f7f9);
    border: solid #d0d6e1 1px;
    border-radius: 0;
    font-size: 16px;
    line-height: 20px;
    color: #14aebd;
    text-align: center;
    overflow: hidden
}
.page-list span.skip {
    display: none;
    margin-top: -1px;
    padding: 9px 0;
    background: #14aebd;
    color: #fff
}
.page-list span.skip:active {
    opacity: .7
}
.page-list span.stop {
    display: none;
    border-right: none;
    color: #14aebd
}
.page-list span.stop:active {
    opacity: .7
}
.page-list.page-focus a.next {
    display: none
}
.page-list.page-focus a.last {
    display: none
}
.page-list.page-focus span.skip {
    display: block;
    width: 20%
}
.page-list.page-focus span.stop {
    display: block;
    width: 19%
}
.page.page-posting a.next {
    width: 22%;
    border-right: none
}
.page-list.page-posting span.num {
    width: 39%
}
.page-list.page-posting.page-focus a.next {
    display: none
}
.page-list.page-posting.page-focus span.num {
    width: 22%
}
.page-list.page-posting.page-focus span.skip {
    display: block;
    width: 20%
}
.page-list.page-posting.page-focus span.stop {
    display: block;
    width: 19%
}
        
