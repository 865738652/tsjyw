body,
ul,
ol,
li,
dl,
dt,
dd,
p,
h1,
h2,
h3,
h4,
h5,
h6,
form,
fieldset,
img,
div {
    margin: 0px;
    padding: 0px;
    border: 0px;
}

body,
ul,
ol,
li,
p,
form,
fieldset,
table,
td {
    font-family: "宋体";
}

body {
    background: #fff;
    color: #000;
}

td,
li,
select,
input,
textarea,
div {
    font-size: 12px;
}

ul,
dl {
    list-style-type: none;
}

select,
input {
    vertical-align: middle;
}

a:link {
    color: #333;
    text-decoration: none;
}

a:visited {
    color: #333;
    text-decoration: none;
}

a:hover {
    color: #333;
    text-decoration: underline;
}

a:active {
    color: #333;
    text-decoration: none;
}
