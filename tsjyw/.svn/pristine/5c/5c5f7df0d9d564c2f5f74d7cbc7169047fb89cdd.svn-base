#bg .channel {
    width: 168px;
    height: 718px;
    background: #FFF;
    border: 1px solid #a1b3b0;
    padding: 15px 15px 0px 15px;
}

#bg .channel .father {
    width: 100%;
    height: 50px;
    background: #cb6500;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    text-align: left;
    font-size: 18px;
    color: #FFF;
    line-height: 50px;
    letter-spacing: 2px;
    text-indent: 20px;
}

#bg .channel .children {
    padding-top: 10px;
}

#bg .channel .children .selected {
    background: #008bd1;
}

#bg .channel .children .selected a {
    color: #FFF;
}

#bg .channel .children li {
    width: 100%;
    height: 35px;
    line-height: 35px;
    font-size: 16px;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    border-bottom-color: #d1d1d1;
    text-indent: 20px;
}

#bg .list {
    width: 1024px;
    min-height: 660px;
    padding-top: 75px;
}

#bg .list .current_location {
    height: 30px;
    line-height: 30px;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    text-align: right;
    padding-right: 5px;
}

#bg .list .list_border {
    width: 1022px;
    min-height: 628px;
    border: 1px solid #a1b3b0;
    background: #FFF;
}

#bg .list .list_border .list_channel {
    background: url(../images/channel_pic.gif) no-repeat;
    width: 100%;
    height: 48px;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    border-bottom-color: #a1b3b0;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    color: #008bd1;
    line-height: 48px;
    text-indent: 45px;
    font-size: 18px;
    letter-spacing: 2px;
}

#bg .list .list_border .news_list {
    padding: 0px 111px;
    width: 800px;
}

#bg .list .list_border .news_list li {
    background: url(../images/list_point.gif) no-repeat 3px 10px;
    height: 25px;
    line-height: 25px;
    text-indent: 13px;
    border-bottom-width: 1px;
    border-bottom-style: dotted;
    border-bottom-color: #999;
    vertical-align: middle;
}

#bg .list .list_border .news_list .data {
    width: 82px;
    text-align: right;
    padding-right: 5px;
    height: 25px;
}

#bg .list .list_border .news_list .title {
    font-size: 18px;
    font-weight: bold;
    text-align: center;
    line-height: 25px;
    margin: auto;
    padding-bottom: 12px;
}

#bg .list .list_border .news_list .info {
    background: #CCC;
    width: 100%;
    font-size: 12px;
    text-align: center;
    height: 25px;
    line-height: 25px;
    margin: auto;
}

#bg .list .list_border .news_list .content {
    padding-top: 5px;
    font-size: 14px;
    line-height: 30px;
}

#bg .list .list_border .news_list .content p {
    padding: 8px 0px;
}

#bg .list .list_border .pape {
    width: 100%;
    height: 30px;
    padding-top: 14px;
    margin: auto;
    text-align: center;
}
/*--------底部开始--------*/

.foot {
    width: 100%;
    min-width: 1024px;
    height: 150px;
    background: #537180;
}

.foot .foot_pic {
    padding: 45px 0 0 40px;
    width: 58px;
}

.foot .foot_describe {
    padding: 30px 0 0 10px;
    width: 450px;
}

.foot .foot_describe li {
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    height: 18px;
    line-height: 18px;
    color: #FFF;
}

.foot .friendly_link {
    width: 240px;
    height: 165px;
}

.foot .friendly_link .link_name {
    padding-top: 20px;
    color: #FFF;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    font-size: 14px;
    letter-spacing: 2px;
}

.foot .friendly_link li {
    color: #FFF;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    height: 25px;
    line-height: 25px;
    text-indent: 30px;
    letter-spacing: 2px;
}

.foot .friendly_link li a {
    color: #FFF;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
}

