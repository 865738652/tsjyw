/*---------顶部---------*/

.head_top {
    width: 100%;
    min-width: 1024px;
    height: 35px;
    background: #a1a7b3;
    color: #FFF;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    font-size: 12px;
    line-height: 35px;
    overflow: hidden;
}

.head_top .link {
    padding-left: 30px;
    font-size: 12px;
}

.head_top .link a {
    color: #FFF;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
}

.head_top .data {
    padding-right: 30px;
    font-size: 12px;
}

.head_top .map {
    padding-right: 30px;
    font-size: 12px;
}

.head_top .map .img {
    padding: 3px 8px 0px 0px;
    width: 29px;
    height: 29px;
}

.head_top .map .img img {
    width: 29px;
    height: 29px;
}

.head_top .map a {
    color: #FFF;
}
/*--------导航--------*/

.head {
    width: 100%;
    min-width: 1024px;
    background: #537180 url(../images/head_b.gif) repeat-x;
    height: 110px;
}

.head .channel {
    width: 716px;
    margin-top: 78px;
}

.head .channel li {
    float: left;
    list-style-type: none;
    width: 79px;
    text-align: center;
    font-size: 14px;
    font-family: Microsoft Yahei, Helvetica, sans-serif;
    height: 30px;
    line-height: 30px;
}

.head .channel li a {
    color: #FFF;
    display: block;
}

.head .channel li a.active,.head .channel li a:hover {
    background: url(../images/under_line.gif) no-repeat 9px 25px;
    text-decoration: none;
    color: #4afb4b;
    display: block;
}
